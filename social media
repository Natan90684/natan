<body class="bg-blue-50">
  <header class="bg-blue-600 shadow">
    <nav class="container mx-auto p-4 flex justify-between">
      <h1 class="text-2xl font-bold text-white">Natan</h1>
      <div>
        <select id="languageSwitcher" class="text-white bg-blue-600 border-0">
          <option value="en">English</option>
          <option value="es-ES">Spanish (Spain)</option>
          <option value="pt-BR">Portuguese (Brazil)</option>
          <!-- Add other languages here -->
        </select>
      </div>
    </nav>
  </header>

  <main class="container mx-auto mt-6">
    <section class="bg-white p-6 rounded shadow">
      <h2 id="welcome" class="text-xl font-semibold text-blue-800">Welcome to Natan</h2>
      <p id="message" class="text-blue-700">A place to share and connect with friends.</p>
    </section>

    <section class="mt-6">
      <h2 id="postTitle" class="text-lg font-semibold text-blue-800 mb-4">Recent Posts</h2>
      <div class="bg-white p-4 rounded shadow mb-4">
        <h3 id="userPost" class="text-md font-semibold text-blue-600">User1's Post</h3>
        <p id="postMessage" class="text-blue-500">This is the first post on Natan!</p>
      </div>
    </section>
  </main>

  <script>
    const translations = {
      en: {
        welcome: "Welcome to Natan",
        message: "A place to share and connect with friends.",
        postTitle: "Recent Posts",
        userPost: "User1's Post",
        postMessage: "This is the first post on Natan!"
      },
      "es-ES": {
        welcome: "Bienvenido a Natan",
        message: "Un lugar para compartir y conectarse con amigos.",
        postTitle: "Publicaciones recientes",
        userPost: "Publicación de User1",
        postMessage: "¡Este es el primer post en Natan!"
      },
      "pt-BR": {
        welcome: "Bem-vindo ao Natan",
        message: "Um lugar para compartilhar e se conectar com amigos.",
        postTitle: "Postagens recentes",
        userPost: "Post de User1",
        postMessage: "Este é o primeiro post no Natan!"
      }
      // Add more languages here
    };

    $(document).ready(function() {
      $('#languageSwitcher').change(function() {
        const selectedLang = $(this).val();
        $('#welcome').text(translations[selectedLang].welcome);
        $('#message').text(translations[selectedLang].message);
        $('#postTitle').text(translations[selectedLang].postTitle);
        $('#userPost').text(translations[selectedLang].userPost);
        $('#postMessage').text(translations[selectedLang].postMessage);
      });
    });
  </script>
</body>
</html>
