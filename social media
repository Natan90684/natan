    <section class="mb-6">
      <div class="flex items-center bg-white p-4 rounded-lg shadow">
        <input type="text" placeholder="Search for users, posts, hashtags..." class="w-full p-2 border border-blue-300 rounded-lg" id="searchInput">
        <button class="bg-blue-600 text-white py-2 px-4 rounded-lg ml-2 hover:bg-blue-700" onclick="performSearch()">Search</button>
      </div>
    </section>

    <!-- Search Results -->
    <section id="searchResults">
      <h2 class="text-xl font-semibold text-blue-800">Search Results</h2>
      <div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 gap-6 mt-4">
        <!-- Example result for a post -->
        <div class="bg-white p-4 rounded-lg shadow">
          <h3 class="font-bold text-blue-700">Post Title or Hashtag</h3>
          <p class="text-gray-600 mt-2">This is a short description of the post...</p>
          <a href="#" class="text-blue-600 hover:underline mt-4 inline-block">Read more</a>
        </div>

        <!-- Example result for a user -->
        <div class="bg-white p-4 rounded-lg shadow text-center">
          <h3 class="font-bold text-blue-700">Username123</h3>
          <button class="mt-2 bg-blue-600 text-white py-1 px-4 rounded hover:bg-blue-700">Follow</button>
        </div>
      </div>
    </section>
  </main>

  <script>
    function performSearch() {
      const query = document.getElementById('searchInput').value;
      // In a real app, you'd use this query to fetch search results from your server
      alert(`Searching for: ${query}`);
    }
  </script>

