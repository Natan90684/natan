  <script>
    function checkConnection() {
      // Simulate checking for internet connection
      if (navigator.onLine) {
        // If online, redirect to home page
        window.location.href = "/";
      } else {
        // If still offline, show alert (or could show a more user-friendly message)
        alert("Still not connected to the Internet. Please check your settings.");
      }
    }
  </script>
</head>
<body class="bg-gray-100 flex items-center justify-center h-screen">

  <div class="text-center bg-white p-6 rounded-lg shadow-lg">
    <h1 class="text-4xl font-bold text-yellow-600">Whoops!</h1>
    <h2 class="text-2xl font-semibold mt-4">Looks like you are not connected to the Internet.</h2>
    <p class="mt-2">Please try again or check your settings.</p>
    <button onclick="checkConnection()" class="mt-4 inline-block bg-blue-600 text-white py-2 px-4 rounded-lg hover:bg-blue-700">Retry</button>
  </div>

</body>
